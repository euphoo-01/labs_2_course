{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug NASM",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/${fileBasenameNoExtension}",
			"args": [],
			"stopAtEntry": true,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"preLaunchTask": "build-nasm",
			"setupCommands": [
				{
					"description": "Включить pretty-printing для gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			]
		}
	]
}
